import { Router } from 'express';
const router = Router();

// Express Router
router.get('/', (req, res) => {
  // Return uptime
  const uptime = "3 day(s), 2 hour(s), 3 minute(s)";
  res.status(200).send({ uptime });
});

// Start Express Server
const port = process.env.PORT || "3000";
router.listen(port, () => {
  console.log("Server started on port ", port);
}); 